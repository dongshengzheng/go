<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled" value="true"/><!-- 全局映射器启用缓存 -->
        <setting name="useGeneratedKeys" value="true"/>
        <setting name="defaultExecutorType" value="REUSE"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.ctoangels.go.common.modules.sys.entity.Menu" alias="Menu"/>
        <typeAlias type="com.ctoangels.go.common.modules.sys.entity.Button" alias="Button"/>
        <typeAlias type="com.ctoangels.go.common.modules.sys.entity.User" alias="User"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.RepairModelItem" alias="RepairModelItem"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.RepairSpecItem" alias="RepairSpecItem"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.RepairProgItem" alias="RepairProgItem"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.RepairProgDetail" alias="RepairProgDetail"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.ItemCount" alias="ItemCount"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.Param" alias="Param"/>
        <typeAlias type="com.ctoangels.go.common.modules.go.entity.ParamValueVariable" alias="ParamValueVariable"/>
    </typeAliases>
    <!-- 映射map -->
    <mappers>
        <package name="com.ctoangels.go.common.**.mapper"/>
    </mappers>

</configuration>